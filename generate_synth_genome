#This script creates a synthetic genome of randomly drawn bp's, containing non-overlapping inserts of a multi-copy cassete and 10 single copy genes 


#.libPaths(new='~/R/x86_64-pc-linux-gnu-library/3.2')

MakeGenome <- function(genomesize = 1){
  # this function returns a simulated DNA sequence of genomesize bases
  bps <- sample(size = genomesize, x = c("A","C","G","T"), replace = TRUE, prob = c(0.4,0.1,0.1,0.4))
  genomeout <- paste(bps, collapse = "")
  return(genomeout)
}

Synth_genome <- MakeGenome(genomesize = 52000000)


#read in reference genes from S_brevipes
#note, RPB1 and TOP2 and LYS2 were truncated to ~3.9k bp to account for charlength limits. 
S_brevipes_ITS_IR <- ""
S_brevipes_LSU_IR <- ""
S_brevipes_RPB2_IR <- ""
S_brevipes_GAPDH_IR <- ""
S_brevipes_RPB1_IR <- ""
S_brevipes_ELF1_IR <- ""
S_brevipes_GH63_IR <- ""
S_brevipes_MCM7_IR <- ""
S_brevipes_G6PDH_IR <- ""
S_brevipes_MLS_IR <- ""
S_brevipes_LYS2_IR <- ""
S_brevipes_TOP2_IR <- ""


#make multi-copy regions, multi-copy and merge into cassette. 
S_brevipes_multi_IR_60 <- rep(c(S_brevipes_ITS_IR, S_brevipes_LSU_IR), 60)
S_brevipes_multi_IR_60_reps <- paste(S_brevipes_multi_IR_60, collapse = "")


#get length to 1000bp buffer of each gene or cat
len_ITS_LSU <- nchar(S_brevipes_multi_IR_60_reps) + 1000
len_RPB2 <- nchar(S_brevipes_RPB2_IR) + 1000
len_GAPDH <- nchar(S_brevipes_GAPDH_IR) + 1000
len_RPB1 <- nchar(S_brevipes_RPB1_IR) + 1000
len_RPB1 <- nchar(S_brevipes_RPB1_IR) + 1000
len_ELF1 <- nchar(S_brevipes_ELF1_IR) + 1000
len_GH63 <- nchar(S_brevipes_GH63_IR) + 1000
len_MCM7 <- nchar(S_brevipes_MCM7_IR) + 1000
len_G6PDH <- nchar(S_brevipes_G6PDH_IR) + 1000
len_MLS <- nchar(S_brevipes_MLS_IR) + 1000
len_LYS2 <- nchar(S_brevipes_LYS2_IR) + 1000


#insert multi-copy cassette into synthetic genome
Synth_genome_w_multi <- paste((substr(Synth_genome,0,1000)), S_brevipes_multi_IR_60_reps, (substr(Synth_genome, 1001, nchar(Synth_genome))), collapse = "", sep = "")


#insert single-copy genes into synthetic genome
Synth_genome_w_multi_RPB2 <- paste((substr(Synth_genome_w_multi,0,(len_ITS_LSU)+1000)), S_brevipes_RPB2_IR, (substr(Synth_genome_w_multi, ((len_ITS_LSU)+ 1001), nchar(Synth_genome_w_multi))), collapse = "", sep = "")
Synth_genome_w_multi_RPB2_GAPDH <- paste((substr(Synth_genome_w_multi_RPB2,0,(len_ITS_LSU + len_RPB2)+1000)), S_brevipes_GAPDH_IR, (substr(Synth_genome_w_multi_RPB2, ((len_ITS_LSU + len_RPB2)+ 1001), nchar(Synth_genome_w_multi_RPB2))), collapse = "", sep = "")
Synth_genome_w_multi_GAPDH_RPB1 <- paste((substr(Synth_genome_w_multi_RPB2_GAPDH,0,(len_ITS_LSU + len_RPB2 + len_GAPDH)+1000)), S_brevipes_RPB1_IR, (substr(Synth_genome_w_multi_RPB2_GAPDH, ((len_ITS_LSU + len_RPB2 + len_GAPDH)+ 1001), nchar(Synth_genome_w_multi_RPB2_GAPDH))), collapse = "", sep = "")
Synth_genome_w_multi_RPB1_ELF1 <- paste((substr(Synth_genome_w_multi_GAPDH_RPB1,0,(len_ITS_LSU + len_RPB2 + len_GAPDH + len_RPB1)+1000)), S_brevipes_ELF1_IR, (substr(Synth_genome_w_multi_GAPDH_RPB1, ((len_ITS_LSU + len_RPB2 + len_GAPDH + len_RPB1)+ 1001), nchar(Synth_genome_w_multi_GAPDH_RPB1))), collapse = "", sep = "")
Synth_genome_w_multi_ELF1_GH63 <- paste((substr(Synth_genome_w_multi_RPB1_ELF1,0,(len_ITS_LSU + len_RPB2 + len_GAPDH + len_RPB1 + len_ELF1)+1000)), S_brevipes_GH63_IR, (substr(Synth_genome_w_multi_RPB1_ELF1, ((len_ITS_LSU + len_RPB2 + len_GAPDH + len_RPB1 + len_ELF1)+ 1001), nchar(Synth_genome_w_multi_RPB1_ELF1))), collapse = "", sep = "")
Synth_genome_w_multi_GH63_MCM7 <- paste((substr(Synth_genome_w_multi_ELF1_GH63,0,(len_ITS_LSU + len_RPB2 + len_GAPDH + len_RPB1 + len_ELF1 + len_GH63)+1000)), S_brevipes_MCM7_IR, (substr(Synth_genome_w_multi_ELF1_GH63, ((len_ITS_LSU + len_RPB2 + len_GAPDH + len_RPB1 + len_ELF1 + len_GH63)+ 1001), nchar(Synth_genome_w_multi_ELF1_GH63))), collapse = "", sep = "")
Synth_genome_w_multi_MCM7_G6PDH <- paste((substr(Synth_genome_w_multi_GH63_MCM7,0,(len_ITS_LSU + len_RPB2 + len_GAPDH + len_RPB1 + len_ELF1 + len_GH63 + len_MCM7)+1000)), S_brevipes_G6PDH_IR, (substr(Synth_genome_w_multi_GH63_MCM7, ((len_ITS_LSU + len_RPB2 + len_GAPDH + len_RPB1 + len_ELF1 + len_GH63 + len_MCM7)+ 1001), nchar(Synth_genome_w_multi_GH63_MCM7))), collapse = "", sep = "")
Synth_genome_w_multi_G6PDH_MLS <- paste((substr(Synth_genome_w_multi_MCM7_G6PDH,0,(len_ITS_LSU + len_RPB2 + len_GAPDH + len_RPB1 + len_ELF1 + len_GH63 + len_MCM7 + len_G6PDH)+1000)), S_brevipes_MLS_IR, (substr(Synth_genome_w_multi_MCM7_G6PDH, ((len_ITS_LSU + len_RPB2 + len_GAPDH + len_RPB1 + len_ELF1 + len_GH63 + len_MCM7 + len_G6PDH)+ 1001), nchar(Synth_genome_w_multi_MCM7_G6PDH))), collapse = "", sep = "")
Synth_genome_w_multi_MLS_LYS2 <- paste((substr(Synth_genome_w_multi_G6PDH_MLS,0,(len_ITS_LSU + len_RPB2 + len_GAPDH + len_RPB1 + len_ELF1 + len_GH63 + len_MCM7 + len_G6PDH + len_MLS)+1000)), S_brevipes_LYS2_IR, (substr(Synth_genome_w_multi_G6PDH_MLS, ((len_ITS_LSU + len_RPB2 + len_GAPDH + len_RPB1 + len_ELF1 + len_GH63 + len_MCM7 + len_G6PDH + len_MLS)+ 1001), nchar(Synth_genome_w_multi_G6PDH_MLS))), collapse = "", sep = "")
Synth_genome_w_multi_LYS2_TOP2 <- paste((substr(Synth_genome_w_multi_MLS_LYS2,0,(len_ITS_LSU + len_RPB2 + len_GAPDH + len_RPB1 + len_ELF1 + len_GH63 + len_MCM7 + len_G6PDH + len_MLS + len_LYS2)+1000)), S_brevipes_TOP2_IR, (substr(Synth_genome_w_multi_MLS_LYS2, ((len_ITS_LSU + len_RPB2 + len_GAPDH + len_RPB1 + len_ELF1 + len_GH63 + len_MCM7 + len_G6PDH + len_MLS + len_LYS2)+ 1001), nchar(Synth_genome_w_multi_MLS_LYS2))), collapse = "", sep = "")


#colapse out the ""
whole_synth_genome = paste(Synth_genome_w_multi_LYS2_TOP2, collapse="")

#get num of chr. to read file back in as a string
nchar(whole_synth_genome)

#write the synthetic genome to a file
write(whole_synth_genome, file = "synth_genome.txt", append = TRUE)


quit()


